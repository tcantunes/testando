{
  "expo": {
    "name": "Voluntaí",
    "slug": "voluntariado-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#10B981",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voluntai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.voluntai.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {},
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite que o Voluntaí acesse sua localização para mostrar vagas próximas."
        }
      ]
    ]
  }
}
